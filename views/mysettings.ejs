<script>
    <% include partials/viewscript %>
    var comimg=document.getElementById("comimg");
    var comname=document.getElementById("comname");
    var users=document.getElementById("UsersShowBtn");
    var requests=document.getElementById("requestedUserShowBtn");
    let reqqq=new XMLHttpRequest();
    reqqq.open('GET','/getparcommdet');
    reqqq.onload=function()
    {
        var data=JSON.parse(reqqq.responseText);
        comimg.setAttribute("src",data[0].commpath);
        comname.innerHTML=data[0].commname;
        users.innerHTML="users ("+data[0].users.length+")";
        requests.innerHTML="Requests ("+data[0].requested.length+")";
    }
    reqqq.send();
</script>
